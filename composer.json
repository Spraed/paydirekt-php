{
  "name": "spraed/paydirekt-php",
  "description": "PHP package to communicate with the Paydirekt merchant api",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "DerStoffel",
      "email": "derstoffel@posteo.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Spraed\\Client\\": [
        "src/Spraed/Client/",
        "test/Spraed/Client/"
